    "ng": "ng",
    "start": "NODE_OPTIONS=\"--max-http-header-size=120000\" ng serve --host 0.0.0.0 --port 4201 --proxy-config ./src/proxy.conf.json",
    "clean": "cp dist/ve.0c01b08dda1727ac.svg dist/ve.0c01b08dda1727ac.svgX && cp dist/ve.affb5563508af8a5.svg dist/ve.affb5563508af8a5.svgX && cp dist/us.817d241614dff6d8.svg dist/us.817d241614dff6d8.svgX && cp dist/us.8802c010a1c68b93.svg dist/us.8802c010a1c68b93.svgX && rm -rf dist/*.svg && mv dist/us.8802c010a1c68b93.svgX dist/us.8802c010a1c68b93.svg && mv dist/us.817d241614dff6d8.svgX dist/us.817d241614dff6d8.svg && mv dist/ve.affb5563508af8a5.svgX dist/ve.affb5563508af8a5.svg && mv dist/ve.0c01b08dda1727ac.svgX dist/ve.0c01b08dda1727ac.svg",
    "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
    "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "build-stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --stats-json",
    "build:prod": "node build.js && node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production && npm run clean",
    "build:prod-stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production --stats-json",
    "bundle-analyzer": "webpack-bundle-analyzer dist/vuexy/stats.json",
    "android": "npx cap add android",
    "ios": "npx cap add ios",
    "icons-ios": "cordova-res ios --skip-config --copy",
    "icons-android": "cordova-res android --skip-config --copy",
    "cambiar-icons": "capacitor-resources -p android,ios",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
